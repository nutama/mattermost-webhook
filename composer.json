    "require-dev": {
        "phpunit/phpunit": "^8.5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Utama",
            "email": "nathan@nutama.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2||^8.0",
        "guzzlehttp/guzzle": "^6.2||^7.0"
    },
    "autoload": {
        "psr-4": {
            "Nutama\\MattermostWebhook\\": "src/"
        }
